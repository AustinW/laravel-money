    "keywords": [ "Money", "Value Object", "Generic Sub-domain" ],
    "homepage": "http://blog.verraes.net/2011/04/fowler-money-pattern-in-php/",
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Verraes",
            "email": "mathias@verraes.net"
        }
    ],
    "require": {
